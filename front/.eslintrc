{
  "parserOptions" :{
    "ecamVersion" : 2022,
    "sourceType":"module",
    "ecamFeatures":{
      "jsx" : true
    }
  },
  "env" : {
    "browser":true,
    "node":true,
    "ES6":true
  },
  "extends" : [
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "plugins":[
    "import",
    "react",
    "react-hooks"
  ]
}